container_commands:
  01_migrate:
    command: "python mysite/mysite/manage.py migrate"
    leader_only: true
  02_collectstatic:
    command: "python mysite/mysite/manage.py collectstatic --noinput"
  03_createsu:
    command: "python mysite/mysite/manage.py createsu"
    leader_only: trueoption_settings:
  "aws:elasticbeanstalk:application:environment":
    DJANGO_SETTINGS_MODULE: "mysite/mysite/mysite.settings"
    "PYTHONPATH": "/opt/python/current/app/mysite:$PYTHONPATH"
    "ALLOWED_HOSTS": ".elasticbeanstalk.com"
  "aws:elasticbeanstalk:container:python":
    WSGIPath: "mysite/mysite/mysite/wsgi.py"
    StaticFiles: "/static/=mysite/static/"